{
  "name": "template",
  "version": "0.1.0",
  "private": true,
  "description": "Gatsby, the template engine for bristol website",
  "scripts": {
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "clean": "gatsby clean",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "start": "npm run develop",
    "develop": "nodemon --exec 'kill-port --port 8000 && gatsby develop' --watch gatsby-node.js --watch package.json --watch gatsby-config.js --watch gatsby-browser.js"
  },
  "dependencies": {
    "@brainhubeu/react-carousel": "^1.19.16",
    "@emotion/cache": "^10.0.29",
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "babel-plugin-styled-components": "^1.10.7",
    "emotion-theming": "^10.0.27",
    "gatsby": "^2.23.14",
    "gatsby-plugin-emotion": "^4.3.9",
    "gatsby-plugin-google-fonts": "^1.0.1",
    "gatsby-plugin-react-helmet": "^3.3.9",
    "gatsby-plugin-sass": "^2.3.9",
    "gatsby-plugin-styled-components": "^3.3.9",
    "gatsby-plugin-typography": "^2.5.9",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-elastic-carousel": "^0.6.4",
    "react-helmet": "^6.1.0",
    "react-icons": "^3.10.0",
    "react-typography": "^0.16.19",
    "styled-components": "^5.1.1",
    "typography": "^0.16.19",
    "typography-theme-bootstrap": "^0.16.19"
  },
  "devDependencies": {
    "kill-port": "^1.6.0",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5"
  }
}
