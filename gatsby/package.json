{
  "name": "gatsby",
  "version": "1.2.0",
  "private": "true",
  "description": "Gatsby template framework for bristol's website",
  "scripts": {
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "clean": "gatsby clean",
    "build": "GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build --log-pages",
    "serve": "gatsby serve",
    "start": "yarn run develop",
    "develop": "nodemon --exec 'kill-port --port 8000 && gatsby develop -H 0.0.0.0' --watch gatsby-node.js --watch package.json --watch gatsby-config.js --watch gatsby-browser.js"
  },
  "dependencies": {
    "@emotion/cache": "^11.0.0",
    "@emotion/react": "^11.0.0",
    "@emotion/server": "^11.0.0",
    "@emotion/styled": "^11.0.0",
    "gatsby": "^2.23.14",
    "gatsby-alias-imports": "^1.0.4",
    "gatsby-image": "^2.4.13",
    "gatsby-plugin-emotion": "^5.0.0",
    "gatsby-plugin-env-variables": "^2.0.0",
    "gatsby-plugin-google-analytics": "^2.3.13",
    "gatsby-plugin-hotjar": "^1.0.1",
    "gatsby-plugin-layout": "^1.3.10",
    "gatsby-plugin-mailchimp": "^5.2.2",
    "gatsby-plugin-manifest": "^2.4.18",
    "gatsby-plugin-offline": "^3.2.17",
    "gatsby-plugin-react-helmet": "^3.3.10",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-scroll-reveal": "^0.0.7",
    "gatsby-plugin-sharp": "^2.6.19",
    "gatsby-plugin-typography": "^2.5.9",
    "gatsby-plugin-web-font-loader": "^1.0.4",
    "gatsby-source-contentful": "^4.0.0",
    "gatsby-source-filesystem": "^2.3.19",
    "gatsby-transformer-sharp": "^2.5.11",
    "gatsby-transformer-yaml": "^2.4.10",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.0.0",
    "react-slick": "^0.27.0",
    "react-typography": "^0.16.19",
    "slick-carousel": "^1.8.1",
    "typography": "^0.16.19",
    "typography-theme-bootstrap": "^0.16.19"
  },
  "devDependencies": {
    "kill-port": "^1.6.0",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5"
  }
}
